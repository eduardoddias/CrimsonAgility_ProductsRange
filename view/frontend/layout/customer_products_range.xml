<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <title>Products In Range</title>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Products In Range</argument>
            </action>
        </referenceBlock>
        <move element="messages" destination="content" before="-"/>
        <referenceContainer name="content">
            <block name="products_range" template="CrimsonAgility_ProductsRange::index.phtml"/>
        </referenceContainer>
    </body>
</page>
